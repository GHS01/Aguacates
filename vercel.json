{
  "version": 2,
  "builds": [
    {
      "src": "server/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/public"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.ts"
    },
    {
      "src": "/fonts/(.*)",
      "dest": "/dist/public/fonts/$1",
      "continue": true
    },
    {
      "src": "/images/(.*)",
      "dest": "/dist/public/images/$1",
      "continue": true
    },
    {
      "src": "/assets/(.*)",
      "dest": "/dist/public/assets/$1",
      "continue": true
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/server/index.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
